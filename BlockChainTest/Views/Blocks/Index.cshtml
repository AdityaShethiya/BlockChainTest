@model IEnumerable<BlockChainTest.Models.Block>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.timpStamp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.previousHash)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.hash)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nonce)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.timpStamp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.previousHash)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.hash)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nonce)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.BlockId }) |
            @Html.ActionLink("Details", "Details", new { id=item.BlockId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.BlockId })
        </td>
    </tr>
}

</table>
